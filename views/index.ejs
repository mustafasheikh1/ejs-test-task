<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <div class="obj1">
      <button class="btn-obj1" id="option">Option</button>
      <div class="btn-obj1" id="option">
        <p>Quantity</p>
        <small>(Press Enter to execute)</samll>
        <input id="quantity" value="<%= quantity %>" min="1" type="number" style="margin-top: 0.5rem; background-color: rgb(160, 160, 160);"/>
      </div>
    </div>
    <div id="mySidenav" class="obj2">
      <select class="dropdown" id="dropdown">
        <option value="A" selected>A</option>
        <option value="B">B</option>
      </select>
    </div>
    <div class="obj3" id="obj3" type="<%= type %>">
      <% for (let i = 0; i < items.length; i++) { %>
        <div class="item">
          <div class="item-index-wrapper">
            <div class="item-index"><%= items[i] %></div>
          </div>
          <div class="item-content">Item <span id="type"></span><%= items[i] %></div>
        </div>
      <% } %>
      <div class="paginator" id="paginator" limit="<%= limit %>">
        <div class="paginator-option">
          <div class="back" id="back"><</div>
        </div>
        <div class="paginator-option">
          <div class="forward" id="forword">></div>
        </div>
      </div>
    </div>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script type="text/javascript" src="/javascripts/paginator.js"></script>
    <script type="text/javascript" src="/javascripts/sidenav.js"></script>
  </body>
</html>
